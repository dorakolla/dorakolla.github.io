{
    "version": 2,
    "builds": [
      { "src": "/public/**/*", "use": "@vercel/static" },
      { "src": "/script.js", "use": "@vercel/node" },
      { "src": "/server.js", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/public/(.*)", "dest": "/public/$1" },
      { "src": "/script.js", "dest": "/script.js" },
      { "src": "/server.js", "dest": "/server.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "env": {
      "PORT": "3000"
    }
  }
  