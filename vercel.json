{
    "version": 2,
    "builds": [
      {
        "src": "/public/script.js",
        "use": "@vercel/static"
      },
      {
        "src": "/public/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "PORT": "3000"
    }
  }
  