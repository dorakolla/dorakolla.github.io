{
  "version": 2,
  "builds": [
    {
      "src": "/Public/script.js",
      "use": "@vercel/node"
    },
    {
      "src": "/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/Public/(.*)",   
      "dest": "/Public/$1"    
    },
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "PORT": "3000"
  }
}
